        bt5 = types.KeyboardButton('–ü–∞—Å—Ç–∞ —Å –∫—Ä–µ–≤–µ—Ç–∫–∞–º–∏ –≤ —Ç–æ–º–∞—Ç–Ω–æ —Å–ª–∏–≤–æ—á–Ω–æ–º')
        bt6 = types.KeyboardButton('–ü–∞—Å—Ç–∞ —Å —Ç–∞—Ä-—Ç–∞—Ä–æ–º –∏–∑ –∫—Ä–µ–≤–µ—Ç–æ–∫ –∏ —Ü—É–∫–∏–Ω–∏')
        bt7 = types.KeyboardButton('–í–µ—Ä–Ω—É—Ç—å—Å—è –Ω–∞–∑–∞–¥')
        markup.add(bt1, bt2, bt3, bt4, bt5, bt6, bt7)
        bot.send_message(message.from_user.id, "–í—ã–±–µ—Ä–∏—Ç–µ –æ–¥–Ω—É –∏–∑ –ø–æ–∑–∏—Ü–∏–π", reply_markup=markup)

    elif message.text == '–ö–ª–∞—Å—Å–∏—á–µ—Å–∫–∞—è –∫–∞—Ä–±–æ–Ω–∞—Ä–∞ –Ω–∞ –ø–∞—Ä–º–µ–∑–∞–Ω–µ':
        bot.send_message(message.from_user.id, """
        """)

    elif message.text == '–ê—Ä—Ä–∞–±—å—è—Ç–∞ —Å–æ —Å—Ç—Ä–∞—á–∞—Ç–µ–ª–ª–æ–π':

    elif message.text == '–î–æ–º–∞—à–Ω—è—è –ø–µ—Å—Ç–æ —Å–æ —Å—Ç—Ä–∞—á–∞—Ç–µ–ª–ª–æ–π':

    elif message.text == '–ü–∞—Å—Ç–∞ —Å –±–µ–ª—ã–º–∏ –≥—Ä–∏–±–∞–º–∏ –∏ —Ü–∏–ø–ª–µ–Ω–∫–æ–º':

    elif message.text == '–ü–∞—Å—Ç–∞ —Å –∫—Ä–µ–≤–µ—Ç–∫–∞–º–∏ –≤ —Ç–æ–º–∞—Ç–Ω–æ —Å–ª–∏–≤–æ—á–Ω–æ–º':

    elif message.text == '–ü–∞—Å—Ç–∞ —Å —Ç–∞—Ä-—Ç–∞—Ä–æ–º –∏–∑ –∫—Ä–µ–≤–µ—Ç–æ–∫ –∏ —Ü—É–∫–∏–Ω–∏':


bot.polling(none_stop=True, interval=0)
