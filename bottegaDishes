import telebot
from telebot import types
import random
from random import choice

bot = telebot.TeleBot('6261930695:AAEeU9PkHdQ93njvTCj7iDHgbxE1eR3ILIQ')
@bot.message_handler(commands=["start"])
def start(message):

    markup = types.ReplyKeyboardMarkup(resize_keyboard= True)
    btn1 = types.KeyboardButton('–ë–∞—Äüç∫')
    btn2 = types.KeyboardButton('–ö—É—Ö–Ω—èü•ó')
    markup.add(btn1, btn2)
    bot.send_message(message.from_user.id, "–í—ã–±–µ—Ä–∏—Ç–µ –±–∞—Ä –ª–∏–±–æ –∫—É—Ö–Ω—é", reply_markup= markup)

